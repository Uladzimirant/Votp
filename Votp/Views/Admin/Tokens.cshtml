@using Votp.Models.Request;
@model IEnumerable<Votp.Models.Response.TokenODto>


@{
    ViewData["Title"] = "Tokens";
}

<h1>Tokens</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Token)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.UserName)
            </th>
            <th></th>
        </tr>
    </thead>
    <form asp-action="SelectionAction">
    <tbody>
@foreach (var item in Model) { 
        <tr>

            <td>
                @Html.DisplayFor(modelItem => item.Token)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.UserName)
            </td>
            <td>
                <input type="checkbox" id=@item.Id name="Tokens" value=@item.Id>
            </td>
        </tr>
}
    </tbody>
    <div class="form-group">
        <input type="submit" value="Select" class="btn btn-primary" />
    </div>
    </form>
